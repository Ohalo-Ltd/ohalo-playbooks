# Copy to .env and update with real credentials before running the sync service.

# Data X-Ray configuration
DXR_BASE_URL=https://atlan.dataxray.io/api
DXR_PAT=replace-me
# Comma-separated classification types to ingest (leave blank for all)
DXR_CLASSIFICATION_TYPES=LABEL,ANNOTATOR
DXR_SAMPLE_FILE_LIMIT=5
DXR_FILE_FETCH_LIMIT=200

# Atlan configuration
# Base URL for the Atlan tenant (scheme + host, trailing slash optional)
ATLAN_BASE_URL=https://ohalo-partner.atlan.com/
# API token with connection-admin permissions for the target tenant
ATLAN_API_TOKEN=replace-me
ATLAN_GLOBAL_CONNECTION_QUALIFIED_NAME=default/custom/dxr-unstructured-attributes
ATLAN_GLOBAL_CONNECTION_NAME=dxr-unstructured-attributes
# Optional Atlan domain assigned to the global connection
ATLAN_GLOBAL_DOMAIN="DXR Unstructured"
# Connector name displayed for the global connection (e.g. custom)
ATLAN_GLOBAL_CONNECTOR_NAME=custom
ATLAN_DATABASE_NAME=dxr
ATLAN_SCHEMA_NAME=labels
# Maximum number of assets per save request
ATLAN_BATCH_SIZE=20
# Optional suffix appended to dataset qualified names
ATLAN_DATASET_PATH_PREFIX=dxr
# Prefix applied to per-datasource connection names
ATLAN_DATASOURCE_CONNECTION_PREFIX=dxr-datasource
# Optional domain prefix for per-datasource connections
ATLAN_DATASOURCE_DOMAIN_PREFIX=DXR
# Namespace prefix used when creating Atlan tag definitions
ATLAN_TAG_NAMESPACE=DXR
# Optional service account (user) ID to add as a connection admin when creating the connection
# e.g. service-account-apikey-23495327-3cb4-431d-8a33-32fbd143c33a
# ATLAN_CONNECTION_ADMIN_USER="dev env token"

# Logging configuration
LOG_LEVEL=INFO
